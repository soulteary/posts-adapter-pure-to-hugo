{
  "//": "忽略的文件列表",
  "//": "可以填写`20XX`等年月日归档目录",
  "blackList": [
    ".DS_Store",
    ".git",
    ".gitignore",
    "README.json",
    "README.md"
  ],
  "//": "时间转换使用的时区",
  "timezone": "Asia/Shanghai",
  "//": "读写文件使用的编码",
  "charset": "utf-8",
  "//": "调试信息",
  "debug": {
    "enable": false,
    "maxFilesCount": 11,
    "showCategory": false
  },
  "//": "外部代码高亮接口地址",
  "codeHighlight": {
    "api": "http://crayon/?api",
    "remote": true
  },
  "//": "缓存目录",
  "cache": {
    "rootDir": "./cache",
    "database": "./cache.json"
  },
  "//": "并发处理",
  "concurrence": 10,
  "//": "要转换的数据源",
  "jobs": [
    {
      "source": "../My-Blog-Posts",
      "hl": true,
      "dist": "../blog-2017/content"
    }
  ]
}
